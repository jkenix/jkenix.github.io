  "scripts": {
    "start": "cross-env SERVE=true webpack serve",
    "build": "webpack",
    "build-prod": "webpack --mode=production",
    "clean": "rd /s /q docs",
    "dev": "run-p layout media base theme state module",
    "webstart": "webpack serve --open",
    "webdev": "set NODE_ENV=development&&webpack",
    "webbuild": "set NODE_ENV=production&&webpack",
    "parcel": "parcel index.html",
    "parcelbuild": "parcel build src/index.html --no-optimize",
    "eslint": "eslint --init",
    "base": "sass --watch src/styles/base.scss:src/styles/css/base.css",
    "layout": "sass --watch src/styles/layout.scss:src/styles/css/layout.css",
    "module": "sass --watch src/styles/module.scss:src/styles/css/module.css",
    "state": "sass --watch src/styles/state.scss:src/styles/css/state.css",
    "theme": "sass --watch src/styles/theme.scss:src/styles/css/theme.css",
    "media": "sass --watch src/styles/media.scss:src/styles/css/media.css"
  }
